package com.ehr;

import org.apache.log4j.Logger;

public class SamsungTV implements Tv {
	
	private String brandNm = "SamsungTV";
	@Override
	public void powerOn() {
		LOG.debug("------------------------------------");
		LOG.debug("powerOn : "+brandNm+" TV의 전원을 켠다.");
		LOG.debug("------------------------------------");
	}

	@Override
	public void powerOff() {
		LOG.debug("------------------------------------");
		LOG.debug("powerOn : "+brandNm+" TV의 전원을 켠다.");
		LOG.debug("------------------------------------");
	}

	@Override
	public void volumeUp() {
		LOG.debug("------------------------------------");
		LOG.debug("powerOn : "+brandNm+" TV의 볼륨을 높인다.");
		LOG.debug("------------------------------------");
	}

	@Override
	public void volumeDown() {
		LOG.debug("------------------------------------");
		LOG.debug("powerOn : "+brandNm+" TV의 볼륨을 높인다.");
		LOG.debug("------------------------------------");
	}

}
